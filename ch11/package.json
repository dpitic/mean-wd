{
  "name": "MEAN",
  "version": "0.0.11",
  "description": "Example MEAN project using the horizontal folder structure",
  "scripts": {
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "app": "node server",
    "app:dev": "npm run lint && npm run app",
    "nodemon": "nodemon -w app -w config -w server.js --exec npm run app:dev",
    "start": "concurrently \"npm run webpack\" \"npm run nodemon\"",
    "debug": "node --inspect --debug-brk server.js",
    "postinstall": "typings install",
    "webpack": "webpack --watch",
    "lint": "eslint --ext .js ./config ./app ./*.js"
  },
  "dependencies": {
    "@angular/common": "2.4.10",
    "@angular/compiler": "2.4.10",
    "@angular/core": "2.4.10",
    "@angular/forms": "2.4.10",
    "@angular/http": "2.4.10",
    "@angular/platform-browser": "2.4.10",
    "@angular/platform-browser-dynamic": "2.4.10",
    "@angular/router": "3.4.10",
    "body-parser": "1.17.1",
    "core-js": "2.4.1",
    "compression": "1.6.2",
    "connect-flash": "0.1.1",
    "connect-mongo": "1.3.2",
    "cookie-parser": "1.4.3",
    "ejs": "2.5.6",
    "es6-promise": "4.1.0",
    "express": "4.15.2",
    "express-session": "1.15.1",
    "method-override": "2.3.7",
    "mongoose": "4.9.0",
    "morgan": "1.8.1",
    "passport": "0.3.2",
    "passport-facebook": "2.1.1",
    "passport-google-oauth": "1.0.0",
    "passport-local": "1.0.0",
    "passport-twitter": "1.0.4",
    "reflect-metadata": "0.1.10",
    "rxjs": "5.2.0",
    "socket.io": "1.7.3",
    "systemjs": "0.20.10",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "awesome-typescript-loader": "3.1.2",
    "concurrently": "3.4.0",
    "eslint": "3.10.2",
    "jasmine": "2.5.3",
    "jasmine-core": "2.5.2",
    "karma": "1.5.0",
    "karma-jasmine": "1.1.0",
    "karma-phantomjs-launcher": "1.0.4",
    "nodemon": "1.11.0",
    "should": "11.2.1",
    "supertest": "3.0.0",
    "traceur": "0.0.111",
    "typescript": "2.2.1",
    "typings": "2.1.0",
    "webpack": "1.13.3"
  }
}
